{
	"name": "@markwylde/ailib",
	"version": "1.0.0",
	"description": "An AI library for calling OpenRouter in JavaScript",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc",
		"prepublishOnly": "npm run build",
		"lint": "biome check .",
