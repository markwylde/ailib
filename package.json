{
	"name": "@markwylde/ailib",
	"version": "1.5.1",
	"description": "An AI library for calling OpenRouter in JavaScript",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc",
		"prepublishOnly": "npm run build",
		"check": "biome check .",
		"check:fix": "biome check . --write",
		"lint": "biome lint .",
		"lint:fix": "biome lint . --write",
		"tidy": "npm run check:fix && npm run lint:fix",
		"example:weather": "npm run build && tsx examples/weather.ts",
		"example:conversation": "npm run build && tsx examples/conversation.ts",
		"example:structured-output": "npm run build && tsx examples/structured-output.ts",
		"example:ollama:tools": "npm run build && tsx examples/ollama-tools.ts",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [
		"ai",
		"openai",
		"openrouter",
		"llm"
	],
	"author": {
		"name": "Mark Wylde",
		"email": "mark@wylde.net",
		"url": "https://markwylde.com"
	},
	"license": "MIT",
	"type": "module",
	"dependencies": {
		"emittery": "^1.2.0",
		"jsonrepair": "^3.13.1",
		"zod": "^4.1.11"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.5",
		"@types/node": "^24.6.2",
		"dotenv": "^17.2.3",
		"tsx": "4.20.6",
		"typescript": "^5.9.3"
	},
	"files": [
		"dist"
	]
}
