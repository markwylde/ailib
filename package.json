	"dependencies": {
		"emittery": "^1.1.0",
		"zod": "^3.24.4"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "^22.15.17",
		"dotenv": "^16.5.0",
		"typescript": "^5.8.3",
		"tsx": "4.19.4"
	},
	"files": ["dist"]
}
